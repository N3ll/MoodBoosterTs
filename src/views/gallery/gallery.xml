<Page navigatingTo="navigatingTo">
	<Repeater items="{{ arrayOfPictures }}">
		<Repeater.itemsLayout>
			<WrapLayout itemWidth="100" />
		</Repeater.itemsLayout>
		<Repeater.itemTemplate>
			<Image src="{{ Uri }}" stretch="aspectFit"/>
			<Label text="{{ joke }}"/>
		</Repeater.itemTemplate>
	</Repeater>
</Page>